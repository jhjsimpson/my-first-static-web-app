{
  "navigationFallback": {
      "rewrite": "/index.html"
  },
  "routes": [
      {
        "route": "/api/*",
        "methods": ["POST", "PUT"],
        "allowedRoles": ["authenticated"]
      }
  ],
    "responseOverrides": {
      "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/aad"
    }
  }
}