{
  "routes": [
    {
      "route": "/manifest.json",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/aad"
    }
  }
}